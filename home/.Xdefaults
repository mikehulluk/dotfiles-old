URxvt*background: Black
URxvt*foreground: White
URxvt*geometry: 80x30
URxvt*scrollBar: true

#"URxvt.font: 9x15

!-- Xft settings -- !
Xft.dpi:        96
Xft.antialias:  true
Xft.rgba:       rgba
Xft.hinting:    true
Xft.hintstyle:  hintslight



URxvt*scrollTtyKeypress:  true
URxvt*scrollTtyOutput:  false
URxvt*scrollWithBuffer:  false
URxvt*fading:  30
URxvt*saveLines:  65000
URxvt*scrollBar_right:true
URxvt*transpscrollbar:true
URxvt*shading:40









! -- Fonts -- !
# URxvt.font:xft:Monospace:pixelsize=15
# URxvt.boldfont:xft:Monospace-Bold:pixelsize=15
! Using Xft

#URxvt*font:           xft:Inconsolata:size=10:antialias=true:hinting=slight
#URxvt*font:               xft:Inconsolata:style=Semibold:pixelsize=14:antialias=true:hinting=slight
#URxvt*boldFont:           xft:Inconsolata:style=bold:pixelsize=14:antialias=true:hinting=slight
#URxvt.font: -*-terminus-medium-*-*-*-*-140-*-*-*-*-iso8859-1

#urxvt*font: xft:Terminus:size=10
#urxvt*boldFont: xft:Terminus:style=bold

URxvt.font:     xft:bitstream vera sans mono:size=9:antialias=true
URxvt.boldFont: xft:bitstream vera sans mono:bold:size=9:antialias=true

URxvt.letterSpace: -2


*foreground: #bfa876
*background: #2b1b10

